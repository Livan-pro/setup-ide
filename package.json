{
	"name": "setup-ide",
	"version": "1.1.1",
	"keywords": [
		"setup",
		"ide",
		"cli",
		"vscode",
		"webstorm",
		"intellij"
	],
	"homepage": "https://github.com/Livan-pro/setup-ide#readme",
	"bugs": {
		"url": "https://github.com/Livan-pro/setup-ide/issues"
	},
	"repository": "https://github.com/Livan-pro/setup-ide",
	"license": "MIT",
	"author": "Livan.pro <me@livan.pro> (https://github.com/Livan-pro)",
	"type": "module",
	"bin": "./dist/cli.mjs",
	"scripts": {
		"build": "pkgroll",
		"dev": "tsx src/cli.ts",
		"format": "prettier --check .",
		"format-fix": "prettier --write .",
		"lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
		"lint-fix": "eslint --report-unused-disable-directives --fix .",
		"prepublish": "yarn build",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.0",
		"@tsconfig/node22": "^22.0.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/node": "^22.9.3",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"pkgroll": "^2.5.1",
		"prettier": "^3.3.3",
		"prettier-plugin-packagejson": "^2.5.5",
		"tsx": "^4.19.2",
		"typescript": "~5.6.3",
		"typescript-eslint": "^8.15.0"
	},
	"packageManager": "yarn@4.5.2",
	"engines": {
		"node": ">=22.0.0"
	}
}
